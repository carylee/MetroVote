%ul.breadcrumb
  %li
    %a{:href => '/'} MetroVote
    %span.divider /
  %li.active Seattle City Council
.container
  .content
    - @positions.each do |position|
      - if position.candidates.length > 1
        %a.position{:href => '/positions/' + position.id.to_s}
          %span.left.name= position.candidates[0].name
          %span.right.name= position.candidates[1].name
          %span.number= position.region_number
